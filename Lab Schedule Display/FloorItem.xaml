<UserControl
    x:Class="Lab_Schedule_Display.FloorItem"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Lab_Schedule_Display"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" Height="192" Width="332" Background="DarkGray">

    <Grid Background="DarkGray">
        <StackPanel Margin="24,28,0,0">
            <TextBlock x:Name="floorLevel" Text="" FontSize="32" Style="{ThemeResource TitleTextBlockStyle}" />
            <TextBlock x:Name="location" Text="" FontSize="20" />
        </StackPanel>
    </Grid>
</UserControl>
